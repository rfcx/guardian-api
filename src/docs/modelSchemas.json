{
  "Stream": {
    "properties": {
      "id": {
        "type": "string",
        "example": "01a2ucztj8ax"
      },
      "name": {
        "type": "string",
        "example": "My Stream 1"
      },
      "latitude": {
        "type": "float",
        "example": 90.123
      },
      "longitude": {
        "type": "float",
        "example": 125.912
      },
      "project": {
        "type": "object",
        "$ref": "#/components/schemas/Project"
      }
    }
  },
  "StreamWithEventsCount": {
    "allOf": [
      {
        "$ref": "#/components/schemas/Stream"
      },
      {
        "properties": {
          "eventsCount": {
            "type": "number",
            "example": 10
          }
        }
      }
    ]
  },
  "Project": {
    "properties": {
      "id": {
        "type": "string",
        "example": "xu82jDgX49"
      },
      "name": {
        "type": "string",
        "example": "My Project 1"
      },
      "isPublic": {
        "type": "boolean",
        "example": true
      },
      "externalId": {
        "type": "number",
        "example": 1910
      }
    }
  }
}
